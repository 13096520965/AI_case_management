# 数据库配置
DB_PATH=./data/legal_case.db

# JWT配置
JWT_SECRET=your-secret-key-here
JWT_EXPIRES_IN=7d

# 服务器配置
PORT=3000
NODE_ENV=development

# AI服务配置
# 可选值: mock, openai, qianwen, wenxin
AI_PROVIDER=mock

# OpenAI配置（如果使用OpenAI）
# AI_PROVIDER=openai
# AI_API_KEY=sk-your-openai-api-key
# AI_API_URL=https://api.openai.com/v1/chat/completions
# AI_MODEL=gpt-3.5-turbo
# AI_MAX_TOKENS=2000
# AI_TIMEOUT=30000

# 通义千问配置（如果使用通义千问）
# AI_PROVIDER=qianwen
# AI_API_KEY=your-qianwen-api-key
# AI_API_URL=https://dashscope.aliyuncs.com/api/v1/services/aigc/text-generation/generation
# AI_MODEL=qwen-turbo
# AI_MAX_TOKENS=2000
# AI_TIMEOUT=30000

# 文心一言配置（如果使用文心一言）
